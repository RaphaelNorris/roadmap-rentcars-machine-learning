# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# S3 Buckets
S3_RAW_BUCKET=your-raw-data-bucket
S3_PROCESSED_BUCKET=your-processed-data-bucket
S3_ML_ARTIFACTS_BUCKET=your-ml-artifacts-bucket

# Athena Configuration
ATHENA_DATABASE=ml_database
ATHENA_WORKGROUP=primary
ATHENA_OUTPUT_LOCATION=s3://your-athena-results-bucket/

# EC2 Configuration
EC2_TRAINING_INSTANCE_TYPE=ml.m5.2xlarge
EC2_INFERENCE_INSTANCE_TYPE=ml.t3.medium
EC2_AMI_ID=ami-xxxxxxxxxx
EC2_KEY_NAME=your-key-pair
EC2_SECURITY_GROUP=sg-xxxxxxxxxx
EC2_IAM_ROLE=your-ml-role

# ECR Configuration
ECR_REGISTRY_URL=123456789012.dkr.ecr.us-east-1.amazonaws.com

# MLFlow Configuration
MLFLOW_TRACKING_URI=http://mlflow-server:5000
MLFLOW_EXPERIMENT_NAME=default-experiment
MLFLOW_S3_ENDPOINT_URL=https://s3.us-east-1.amazonaws.com

# Airflow Configuration
AIRFLOW_HOME=/opt/airflow
AIRFLOW__CORE__DAGS_FOLDER=/opt/airflow/dags
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@postgres:5432/airflow

# Feature Store
FEAST_REGISTRY_PATH=s3://your-processed-data-bucket/feature_store/registry.db

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# Model Serving
MODEL_SERVING_PORT=8000
MODEL_SERVING_HOST=0.0.0.0

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Environment
ENVIRONMENT=development
PROJECT_NAME={{ cookiecutter.project_name }}
